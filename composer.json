{
    "name": "shineoncom/laravel-zoho-api",
    "description": "ZOHO API with Laravel integrations using latest Guzzle.",
    "license": "MIT",
    "keywords": ["zoho", "api", "webhooks", "laravel"],
    "type": "project",
    "authors": [
        {
            "name": "Dan Richards",
            "email": "danrichardsri@gmail.com"
        },
        {
            "name": "Diogo Gomes",
            "email": "diogogomeswww@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4": {
            "ShineOnCom\\Zoho\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2",
        "nesbot/carbon": "^1.26.3 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3"
    },
    "autoload-dev": {
        "psr-4": {
            "ShineOnCom\\Zoho\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShineOnCom\\Zoho\\Integrations\\Laravel\\ZohoServiceProvider"
            ]
        }
    }
}
